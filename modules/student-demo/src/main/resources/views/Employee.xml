<?xml version="1.0" encoding="UTF-8"?>
<object-views xmlns="http://axelor.com/xml/ns/object-views"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:schemaLocation="http://axelor.com/xml/ns/object-views http://axelor.com/xml/ns/object-views/object-views_5.4.xsd">
		<grid model="com.axelor.employee.db.Employee" title="Employees" name="employee-grid">
				<field name="ename"/>
				<field name="active"/>
				<field name="roomno"/>
				<field name="salary"/>
				<field name="inDate"/>
				<field name="duration"/>
				<field name="startsOn"/>
				
		</grid>
		<form model="com.axelor.employee.db.Employee" title="Employee" name="employee-form">
				<panel>
					<field name="ename"/>
					<field name="active"/>
					<field name="roomno"/>
					<field name="salary"/>
					<field name="inDate"/>
					<field name="duration"/>
					<field name="startsOn"/>
					
				</panel>
				
		</form>

</object-views>
  